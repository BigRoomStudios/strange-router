{
    "extends": [
        "plugin:@hapi/recommended"
    ],
    "env": {
        "jest": true,
        "commonjs": true,
        "es6": true
    },
    "plugins": ["react"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "react/jsx-uses-react": "error",
        "react/jsx-uses-vars": "error"
    }
}